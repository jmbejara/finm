# Environment configuration for finm package
# Copy this file to .env and customize as needed

# Directory for cached data files
DATA_DIR=/Users/jdoe/GitRepositories/finm/_data

# Directory for build outputs (notebooks, HTML, etc.)
OUTPUT_DIR=/Users/jdoe/GitRepositories/finm/_output

# Operating system type: "nix" (macOS/Linux) or "win" (Windows)
OS_TYPE=nix

# WRDS username (required for pulling data from WRDS)
WRDS_USERNAME=
